FROM apache/superset:latest
USER root
RUN apt-get update && apt-get upgrade -y
RUN apt-get install -y unixodbc unixodbc-dev python3 python3-pip
USER superset
RUN pip3 install sqlalchemy-exasol

EXPOSE 8088 
